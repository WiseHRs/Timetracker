    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Wise\\Timetracker\\": "src"
        }
    },
    "autoload-dev": {
