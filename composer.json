{
    "name": "WiseHRs/Timetracker",
    "type": "library",
    "description": "Wise Timetracker",
    "keywords": [
        "Wise",
        "Timetracker"
    ],
    "homepage": "https://github.com/Wise/Timetracker",
    "license": "MIT",
    "authors": [
        {
            "name": ":Wise"
        }
    ],
    "require": {
